class Solution {
    public int climbStairs(int n) {
        int dapp[] = new int [n+1];
        dapp[0]=1;
        dapp[1]=1;
        for(int i = 2;i<=n;i++){
            dapp[i] = dapp[i-1]+dapp[i-2];//
        }
        return dapp[n];
    }
}
