 class Solution {
     public int singleNumber(int[] nums) {
       int cl = 0;               //  2  2  1
       for(int i : nums){
          cl ^= i;   //
       }
       return cl;  
     }
 }

// Runtime: 1 ms, faster than 100.00% of Java online submissions for Single Number.
// Memory Usage: 42.2 MB, less than 94.91% of Java online submissions for Single Number.
